<!-- debut de head-->
<%- include('head'); -%>
<title>Agence || Kolima  Transfert</title>
<link href="/assets/semantic/lib/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">

  <!-- fin de head-->
  <style>
    #blockmodifier
    {
      display: none;
    }
  </style>
 
</head>
<script src="/assets/semantic/lib/jquery.validate.js"></script>
<script src="/assets/semantic/lib/moment.min.js"></script>

<script src="/assets/semantic/lib/html2pdf.bundle.min.js"></script>
<script src="/assets/semantic/js/jquery.formatCurrency.js" ></script>
<script src="/assets/semantic/js/easy-number-separator.js"></script>


<!-- debut  de menue-->
<%- include('menue'); -%>
    <!--fin de menue end-->
    <!-- **********************************************************************************************************************************************************
        MAIN CONTENT
        *********************************************************************************************************************************************************** -->
    <!-- debut du body-->
    <section id="main-content">
      <section class="wrapper">
      
        <div class="row mt-5">
         
            <div class="col-lg-12  ">
             
              <div class="row account-contant">
                            
                <div class="col-12">
                    <div class="card card-statistics">
                        <div class="card-body p-0">
                            <div class="row no-gutters">
                                
                                <div class="col-xl-12 col-md-12 col-12">
                                    <div class="page-account-form">
                                        <div class="form-titel border-bottom p-3">
                                            <h5 class="mb-0  textcoleur py-2">Historique des Compensations . </h5>
                                            <!--Boite Modale d erreur Taux -->
                                           
                                            

                                            <!--Fin Boite Modale-->
                                          </div>
                                      
                                        <div class="p-4" >
                                                
                                     
        <div class="row mt">
            <div class="col-md-12">
                <div class="">
                  <h4>Historique des compensation  de l'Agence  <%= agence.nomagence %> </h4>
                  <hr>
                  <div id="deuxdaterapport"  class="table-responsive">
                  <table  id="dataTable"  class="table table-striped table-advance table-hover">
                    <thead>
                      <tr>
                        <th> N °</th>
                        <th> Date de la Compensation   </th> 
                        <th>Repport de Solde   </th>
                        <th>Voir détails</th>
                      </tr>
                    </thead>
                    <tbody>
                        
                        
                        <% if(typeof compensehistorik !== "undefined"){ %>
                                
                            <% for(var i= 0 ; i < compensehistorik.length; i++)  { %> 
                           <tr>
                             <td><%= i+1 %></td>
                             <td ><span id="datenn<%= i+1 %>"></span> </td>
                             <td><%= compensehistorik[i].repportsolde %> </td>
                             <td><button class="btn  btn-theme"><a href="/Superadmin/FactureCompensationAgence/<%= compensehistorik[i].idcompense %>/<%= agence.idagence %>"><i class="fa fa-eye"></i></a></button></td>
                            </tr>
                            <script>
                                 var dt = "<%= compensehistorik[i].datecompensation %>";
                                 var datere =  moment(dt).format('DD/MM/YYYY ');             
                                  $("#datenn<%= i+1 %>").html(datere);                                               
                            </script>
                            <% } %>
                            <% } %>
     
                    </tbody>
                  </table>
                  </div>
                </div>
                <!-- /content-panel -->
              </div>
             
            <!-- /col-lg-12 -->
          </div>
        <!-- /row -->

                                        </div>
                                    </div>
                                </div>
                                
                            </div>
                        </div>
                    </div>
                </div>
            </div>
      <!--Fin de notification d'erreur-->
     
            </div>
            <!-- /col-lg-12 -->
          </div>
          
          <!-- Button trigger modal -->

        <!-- /row -->
      </section>
    </section>
    <!--main content end-->
    
  </section>
  <!-- js placed at the end of the document so the pages load faster -->
  
<script src="/assets/semantic/lib/datatables/jquery.dataTables.min.js"></script>
